<% @songs.each do |song| %>
  <tr>
    <td><%= song.artist %></td>
    <td>
      <a href="/songs/<%= song.id %>">
        <%= song.title %>
      </a>
    </td>
    <td><%= song.playlist_adds.count %></td>
    <td>

      <form action="/songs/<%= song.id %>" method="post">
        <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
        <input class="btn btn-default"type="submit" name="" value="Add">
      </form>

    </td>
  </tr>
<% end %>
<a href="/songs">Head on back!</a>