<a class="btn btn-default nav-button"href="/songs">Dashboard</a>

<h1><%= @user.first_name %> <%= @user.last_name %>'s Playlist</h1>

<table class="table table-striped table-bordered">
  <tr>
    <th>Artist</th>
    <th>Title</th>
    <th>Times Added</th>
  </tr>
  <% @songs.each do |song| %>
    <tr>
      <td><%= song.artist %></td>
      <td>
        <a href="/songs/<%= song.id %>">
          <%= song.title %>
        </a>
      </td>
      <td><%= song.count %></td>
    </tr>
  <% end %>
</table>