<a class="btn btn-default nav-button" href="/songs">Dashboard</a>

<h2>Others who added <%= @song.artist %> - <%= @song.title %></h2>

<table class="table table-striped table-bordered">
  <tr>
    <th>Name</th>
    <th>Times Added</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><a href="/users/<%= user.id %>"><%= user.first_name %> <%= user.last_name %></a></td>
      <td><%= user.times_added %></td>
    </tr>
  <% end %>
</table>